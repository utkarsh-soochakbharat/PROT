{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "GS26/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/GS26/(.*)",
      "dest": "/GS26/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/verify.js": {
      "maxDuration": 10
    },
    "api/random-code.js": {
      "maxDuration": 10
    },
    "GS26/QR.js": {
      "maxDuration": 10
    }
  }
}
